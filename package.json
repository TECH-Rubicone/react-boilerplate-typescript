{
  "name": "react-boilerplate-typescript",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "bootstrap": "^4.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-scripts": "4.0.3",
    "redux": "^4.1.1",
    "redux-saga": "^1.1.3",
    "redux-saga-controller": "^1.1.1",
    "reactstrap": "^8.10.0",
    "typescript": "^4.4.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "eslint": "eslint src",
    "eslint:fix": "eslint src --fix",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --passWithNoTests",
    "eject": "react-scripts eject",
    "start:env": "npm start",
    "start:development": "cross-env REACT_APP_ENV=development npm run start:env",
    "start:staging": "cross-env REACT_APP_ENV=staging npm run start:env",
    "start:production": "cross-env REACT_APP_ENV=production npm run start:env",
    "build:env": "npm run build",
    "build:development": "cross-env REACT_APP_ENV=development npm run build:env",
    "build:staging": "cross-env REACT_APP_ENV=staging npm run build:env",
    "build:production": "cross-env REACT_APP_ENV=production npm run build:env",
    "editorconfig:checker": "editorconfig-checker --exclude '.git|node_modules'",
    "check:code": "npm run editorconfig:checker && npm run eslint"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "cross-env": "^7.0.3",
    "editorconfig-checker": "^4.0.2",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "sass": "^1.39.0"
  }
}
